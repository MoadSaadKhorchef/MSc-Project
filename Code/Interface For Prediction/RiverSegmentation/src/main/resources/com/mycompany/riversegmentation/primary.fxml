<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="453.0" prefWidth="549.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.riversegmentation.PrimaryController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <AnchorPane prefHeight="454.0" prefWidth="550.0">
         <children>
            <Label layoutX="14.0" layoutY="1.0" prefHeight="26.0" prefWidth="216.0" text="Select a photo from your device" textFill="#5c6061">
               <font>
                  <Font name="System Bold" size="11.0" />
               </font>
            </Label>
            <TextField fx:id="pictureNameTextField" alignment="TOP_LEFT" layoutX="14.0" layoutY="34.0" prefHeight="26.0" prefWidth="336.0" style="-fx-background-color: #FFFFFF;" />
            <Button fx:id="browseButton" layoutX="377.0" layoutY="34.0" mnemonicParsing="false" onAction="#browseButtonFunction" prefHeight="26.0" prefWidth="70.0" style="-fx-background-color: #ffffff;" text="Browse" textFill="#5c6061" textOverrun="CLIP" wrapText="true" />
            <Button fx:id="runButton" layoutX="12.0" layoutY="66.0" mnemonicParsing="false" onAction="#runButtonFunction" prefHeight="25.0" prefWidth="70.0" text="Run" />
            <ImageView fitHeight="301.0" fitWidth="264.0" layoutX="14.0" layoutY="98.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
   </children>
</VBox>
